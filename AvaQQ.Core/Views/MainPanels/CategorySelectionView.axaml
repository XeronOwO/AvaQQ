<UserControl xmlns="https://github.com/avaloniaui"
						 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
						 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
						 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
						 mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="40"
						 x:Class="AvaQQ.Core.Views.MainPanels.CategorySelectionView">
	<ScrollViewer x:Name="scrollViewerCategory"
								HorizontalScrollBarVisibility="Hidden"
								PointerWheelChanged="ScrollViewerCategory_PointerWheelChanged">
		<StackPanel x:Name="stackPanelCategory"
								Orientation="Horizontal">
		</StackPanel>
	</ScrollViewer>
</UserControl>
