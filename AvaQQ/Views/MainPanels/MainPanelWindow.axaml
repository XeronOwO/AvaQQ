<Window xmlns="https://github.com/avaloniaui"
				xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
				xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
				xmlns:vm="using:AvaQQ.ViewModels.MainPanels"
				xmlns:mp="clr-namespace:AvaQQ.Views.MainPanels"
				mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="600"
				x:Class="AvaQQ.Views.MainPanels.MainPanelWindow"
				x:DataType="vm:MainPanelViewModel"
				Icon="/Assets/logo.ico"
				Title="AvaQQ"
				Width="{Binding Width}"
				Height="{Binding Height}"
				WindowStartupLocation="CenterScreen">
	<Design.DataContext>
		<vm:MainPanelViewModel/>
	</Design.DataContext>
	<mp:MainPanelView/>
</Window>
